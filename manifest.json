{
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "alarms"],
  "name": "Chrome Extension",
  "version": "0.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
